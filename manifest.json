{
    "manifest_version": 2,
    "name": "AutoFill Form",
    "version": "1.0",
    "description": "AutoFill Forms",
    "icons": {
        "16": "content/images/icon16.png",
        "48": "content/images/icon48.png",
        "64": "content/images/icon64.png",
        "128": "content/images/icon128.png"
    },
    "author": "Roni Chabra",
    "permissions": [
        "activeTab",
        "storage",
        "tabs"
    ],
    "browser_action": {
        "default_icon": "content/images/icon128.png",
        "default_title": "AutoFill Form",
        "default_popup" : "index.html"
    },
    "content_scripts": [
        {
           "matches": ["<all_urls>"],
           "js": ["content/js/site.js"]
        }
     ],
    "background": {
        "page":"background.html"
       // "scripts": ["content/js/background.js"]
      },
      "content_security_policy": "script-src 'self' 'sha256-h2bcw/Q8Yj0sdxXF46xBLBpfEtL5Cdj63HKNUyr+EgQ='; object-src 'self'"
}